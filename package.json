{
  "name": "videditor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "init": "./scripts/init-cloudflare.sh",
    "pages:build": "npx @opennextjs/cloudflare",
    "pages:deploy": "wrangler pages deploy .open-next/worker",
    "worker:dev": "wrangler dev",
    "worker:deploy": "wrangler deploy",
    "d1:migrate:local": "wrangler d1 migrations apply videditor-db --local",
    "d1:migrate:prod": "wrangler d1 migrations apply videditor-db --remote",
    "cf:login": "wrangler login",
    "cf:whoami": "wrangler whoami"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.716.0",
    "@aws-sdk/s3-request-presigner": "^3.716.0",
    "@clerk/nextjs": "^6.14.5",
    "@opennextjs/cloudflare": "^1.12.0",
    "@stripe/stripe-js": "^4.9.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.468.0",
    "next": "^15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-player": "^2.16.0",
    "stripe": "^17.5.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250114.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.3",
    "postcss": "^8.4.49",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "wrangler": "^4.38.0"
  }
}
