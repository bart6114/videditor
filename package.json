{
  "name": "videditor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"next,jobs\" --prefix-colors \"cyan,green\" \"npm:dev:next\" \"npm:dev:jobs\"",
    "dev:next": "next dev",
    "dev:jobs": "cd apps/jobs && uv run python main.py",
    "build": "next build",
    "start": "next start",
    "start:jobs": "cd apps/jobs && uv run python main.py",
    "lint": "next lint",
    "test:upload": "node scripts/test-upload.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "dotenv -e .env.local -- drizzle-kit migrate",
    "db:push": "dotenv -e .env.local -- drizzle-kit push",
    "db:studio": "dotenv -e .env.local -- drizzle-kit studio",
    "db:drop": "drizzle-kit drop",
    "deploy:frontend": "fly deploy --config fly.frontend.toml",
    "deploy:jobs": "fly deploy --config fly.jobs.toml"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.716.0",
    "@aws-sdk/s3-request-presigner": "^3.716.0",
    "@clerk/backend": "^2.23.0",
    "@clerk/nextjs": "^6.14.5",
    "@fastify/cors": "^10.0.1",
    "@fastify/websocket": "^10.0.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-switch": "^1.2.6",
    "@stripe/stripe-js": "^4.9.0",
    "@types/archiver": "^7.0.0",
    "archiver": "^7.0.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.44.7",
    "fastify": "^5.1.0",
    "lucide-react": "^0.468.0",
    "next": "^15.1.3",
    "pg": "^8.13.1",
    "pino": "^9.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-player": "^2.16.0",
    "stripe": "^17.5.0",
    "whisper.cpp": "^1.0.3",
    "zod": "^3.23.8",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "22.19.1",
    "@types/pg": "^8.11.6",
    "@types/react": "19.2.6",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.3",
    "pino-pretty": "^11.2.2",
    "postcss": "^8.4.49",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "5.9.3"
  }
}
